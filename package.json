{
  "name": "memrise-export",
  "description": "Export all words from a Memrise course to a CSV file.",
  "version": "1.1.0",
  "author": {
    "name": "Raine Revere",
    "email": "raine@cybersemics.org",
    "url": "https://github.com/raineorshine"
  },
  "license": "ISC",
  "dependencies": {
    "browserify": "^17.0.0",
    "cheerio": "^1.0.0-rc.3",
    "nodemon": "^2.0.6"
  },
  "scripts": {
    "copy": "mkdir -p build && cp -r src/* build/",
    "browserify": "browserify src/popup.js > build/popup.js",
    "build": "npm run copy && npm run browserify",
    "watch": "nodemon -w src -x \"npm run build\""
